function Setup()
    missionBoard = getObjectFromGUID(missionBoard_GUID)
    eventDeck.shuffle()
    swarmDeck.shuffle()
    rockAndStoneDeck.shuffle()
    resupplyDeck.shuffle()

    -- Mission board setup in the middle of table
    missionBoard.setPosition({0.0,1.0,0.0})
    missionBoard.setLock(true)

    -- Character board setup around mission board
    engineerBoard.setPosition({0.0,1.0,-15.0})
    engineerBoard.setLock(true)

    drillerBoard.setPosition({15.0,1.0,0.0})
    --drillerBoard.rotate({0.0,0.0,0.0})
    drillerBoard.setLock(true)

    gunnerBoard.setPosition({0.0,1.0,15.0})
    --gunnerBoard.rotate({0.0,0.0,0.0})
    gunnerBoard.setLock(true)

    scoutBoard.setPosition({-15.0,1.0,0.0})
    --scoutBoard.rotate({0.0,0.0,0.0})
    scoutBoard.setLock(true)

    -- Removes the button board
    buttonBoard.call('hideBoard')
    -- Switches the mission board to the side without buttons
    missionBoard.setState(1)

    --hideMissionTokens()
end

end

function afterSpawnLock(spawnedObject)
    -- Locks tiles after they spawn
    spawnedObject.lock()
end

-- Shrinks mission tokens so they are not accessible
function hideMissionTokens()
    missionName = {mission1, mission2, mission3, mission4, mission5,
    mission6, mission7, mission8, mission9, mission10, mission11,
    mission12, mission13, mission14}
    for i=1, 14 do
        missionName[i].setScale{0.0,0.0,0.0}
    end

end

